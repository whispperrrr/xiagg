/* pages/start/start.wxss *//* 启动页的 WXSS 文件 */
.startcontainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.logo {
  width: 100px;
  height: 100px;
}

.title {
  margin-top: 20px;
  font-size: 20px;
  color: #333;
}

.full-screen {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
}

