    <t-button  style="position: relative;width: 280px; height: 50px; display: block; box-sizing: border-box; background: rgba(255, 255, 255, 1); border: 1px solid rgba(0, 0, 0, 1); opacity: 1; color: rgba(0, 0, 0, 1); font-size: 50rpx; font-display: medium; text-align: center; line-height: 50px; border-radius: 10px" class="baocun" disabled="{{!nameValue}}" bind:tap="onSubmit">保存</t-button>
