<view class="name-edit" ardstyle="position: relative; left: 65rpx; top: 36rpx" style="border-radius: 12rpx; position: relative; left: 65rpx; top: 74rpx">
  <view class="input-container">
  <t-input
    borderless
    model:value="{{nameValue}}"
    placeholder="请输入文字"
